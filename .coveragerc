[run]
branch = True
source =
    dungeon_nexus

[paths]
source =
    src/dungeon_nexus
    **/site-packages/dungeon_nexus

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
title = dungeon_nexus Coverage Test Report
directory = docs/source/_static/coverage
